local L = game.ReplicatedStorage.Emotes.cursedEnergy
local CEClones = {}
local Clones = 3

local rightLeg = game.Players.LocalPlayer.Character:WaitForChild("Right Leg")
local leftLeg = game.Players.LocalPlayer.Character:WaitForChild("Left Leg")

-- Function to destroy existing clones
local function D()
    for _, clone in ipairs(CEClones) do
        if clone and clone.Parent then
            clone:Destroy()
        end
    end
    CEClones = {}
end

for i = 1, Clones do

    local CELeft = L:Clone()  
    CELeft.Parent = leftLeg
    CELeft.CFrame = leftLeg.CFrame * CFrame.new(0, -0.8, 0)
    CELeft.Anchored = false

    local weldLeft = Instance.new("WeldConstraint")
    weldLeft.Parent = CELeft
    weldLeft.Part0 = leftLeg
    weldLeft.Part1 = CELeft

    table.insert(CEClones, CELeft)

    for _, child in ipairs(CELeft:GetChildren()) do    
        if child:IsA("ParticleEmitter") then
            child:Emit(2)
            child.Enabled = true
        end
    end
end

local function Des()
    coroutine.wrap(function()
        task.wait(1.3)
        D()
    end)()
end

Des()
task.wait(0.5)
if getgenv().secret then
