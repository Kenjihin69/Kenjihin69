spawn(function()
local light = Instance.new("Highlight", game.Players.LocalPlayer.Character)
local cc = Instance.new("ColorCorrectionEffect", game.Lighting)
light.OutlineTransparency = 1
light.FillTransparency = 0
light.FillColor = Color3.new(0, 0, 0)
cc.Brightness = 1
wait(1.4)
light.FillColor = Color3.new(255, 255, 255)
cc.Brightness = -1
wait(0.01)
cc:Destroy()
light:Destroy()
end)
task.wait(0)
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local s = Instance.new("ScreenGui")
s.Parent = playerGui

local textLabel = Instance.new("TextLabel")
textLabel.Parent = s
textLabel.Position = UDim2.new(0.5, -200, 0.8, -50)
textLabel.Size = UDim2.new(0, 400, 0, 50)
textLabel.TextSize = 27
textLabel.TextColor3 = Color3.fromRGB(255, 0, 0) --text color
textLabel.Font = Enum.Font.Antique -- font here
textLabel.BackgroundTransparency = 1
textLabel.Text = "" -- dont change
textLabel.TextStrokeTransparency = 0
textLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0) --stroke color here

local function type(text)
    local index = 1
    while index <= #text do
        textLabel.Text = string.sub(text, 1, index)
        index = index + 1
        wait(0.02)
    end
end

local function f()
    for i = 1, 20 do
        textLabel.TextTransparency = i / 20
        wait(0.01)
    end
end
local function fs()
coroutine.wrap(function()
textLabel.TextStrokeTransparency = 0.1
task.wait(0)
textLabel.TextStrokeTransparency = 0.2
task.wait(0)
textLabel.TextStrokeTransparency = 0.3
task.wait(0)
textLabel.TextStrokeTransparency = 0.4
task.wait(0)
textLabel.TextStrokeTransparency = 0.5
task.wait(0)
textLabel.TextStrokeTransparency = 0.6
task.wait(0)
textLabel.TextStrokeTransparency = 0.7
task.wait(0)
textLabel.TextStrokeTransparency = 0.8
task.wait(0)
textLabel.TextStrokeTransparency = 0.9
task.wait(0)
textLabel.TextStrokeTransparency = 1
end)()
end
coroutine.wrap(function()
    local m = "STAY PROUD." -- text here
    type(m)
    wait(0)
    fs() -- this for stroke lmao 
    f()
end)()
task.wait(1.9)
