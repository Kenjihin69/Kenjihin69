local function stringToCFrame(str)
	local components = {str:match("([^,]+),([^,]+),([^,]+),([^,]+),([^,]+),([^,]+),([^,]+),([^,]+),([^,]+),([^,]+),([^,]+),([^,]+)")}
	for i = 1, #components do
		components[i] = tonumber(components[i])
	end
	return CFrame.new(unpack(components))
end
 
