        local healthGlow = magicHealth.Health.Glow
        local hotbar = LocalPlayer.PlayerGui.Hotbar.Backpack:WaitForChild("Hotbar")

        -- Leave MagicHealth, healthBar, and healthGlow unmodified

        hotbar["1"].Base.ToolName.Text = "Black Flash"
        hotbar["2"].Base.ToolName.Text = "Cursed Barrage"
        hotbar["3"].Base.ToolName.Text = "Manji Kick"
        hotbar["4"].Base.ToolName.Text = "UpperKick"

        magicHealth.TextLabel.Text = "KING OF CURSES"
    end)()
end

