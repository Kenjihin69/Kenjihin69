if effect then
    local character = game.Players.LocalPlayer.Character
    if character then
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        if humanoidRootPart then
            effect.Parent = game.Workspace

            local forwardOffset = 7
            local targetPosition = humanoidRootPart.CFrame * CFrame.new(0, 0,
-forwardOffset)

            for _, part in pairs(effect:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                    part.Anchored = true
                    part.CFrame = targetPosition
                end
            end

            wait(3.3)

            effect:Destroy()
        end
    end
end
wait(0)
spawn(function()
local light = Instance.new("Highlight", game.Players.LocalPlayer.Character)
local cc = Instance.new("ColorCorrectionEffect", game.Lighting)
light.OutlineTransparency = 1
light.FillTransparency = 0
light.FillColor = Color3.new(0, 0, 0)
cc.Brightness = 1
wait(0.05)
light.FillColor = Color3.new(255, 255, 255)
cc.Brightness = -1
wait(0.05)
cc:Destroy()
light:Destroy()
end)
wait(0)
