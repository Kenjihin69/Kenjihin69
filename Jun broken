    while true do
        local hotbar = LocalPlayer.PlayerGui:FindFirstChild("Hotbar") and LocalPlayer.PlayerGui.Hotbar.Backpack:FindFirstChild("Hotbar")
        if hotbar and hotbar:FindFirstChild("1") and hotbar["1"].Base.ToolName.Text == "Normal Punch" then
            test1()
        end
